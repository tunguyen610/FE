                        <div class="post__excerpt ">
                            <div class="excerpt">@obj.Description</div>
                        </div>
                        <div class="post__meta">
                            <span class="entry-author"> By <a class="entry-author__name" href="@obj.PostAccountURL">@obj.PostAccountName</a></span>
